{
	"rules": {
		"no-console": 0,
		"no-underscore-dangle": 0,
		"no-unused-vars": 0,
		"no-use-before-define": 0,
		"no-multi-str": 0,
		"indent": 0,
		"no-trailing-spaces": 0,
		"space-before-function-paren": 0,
		"no-multiple-empty-lines": 0,
        "object-curly-spacing": 0,
        "comma-dangle": 0,
        "no-useless-escape": 0,
        "prefer-template": 0,
        "quotes": 0,
        "no-param-reassign": 0,
        "prefer-destructuring": 0,
        "object-shorthand": 0,
        "no-path-concat": 0,
        "quote-props": 0
	},
	"env": {
		"node": true,
		"mocha": true
	},
	"parserOptions": {
		"ecmaVersion": 8
	},
	"extends": [
		"airbnb-base"
	]
}
